{
  "liveServer.settings.port": 5555,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.fullReload": true,
  "liveServer.settings.ignoreFiles": [
    ".vscode/**",
    "**/*.scss",
    "**/*.sass",
    "**/*.ts",
    "**/*.png",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.gif",
    "**/*.stl",
    "**/*.svg"
  ],
  "javascript.suggest.enabled": true
}
